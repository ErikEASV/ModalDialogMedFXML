<BorderPane id="root" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.modaldialogmedfxml.DialogTestController">
    <center>
        <VBox prefWidth="296.0" spacing="5.0">
            <children>
            <Label text="DialogTest vindue">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font>
            </Label>
                <Label text="Enter an email address:" />
